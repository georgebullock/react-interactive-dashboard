{
  "private": true,
  "name": "react-interactive-dashboard",
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "client": "yarn workspace react-interactive-dashboard-client start",
    "server": "yarn workspace react-interactive-dashboard-server start:dev",
    "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\""
  }
}
