{
	"name": "react-interactive-dashboard-server",
	"version": "1.0.0",
	"description": "React Interactive Dashboard is a project I'm doing to level up my back-of-the-frontend, testing, and React skills",
	"scripts": {
		"start:dev": "nodemon",
		"start:prod": "npm run build && node dist/app.js",
		"build": "rimraf ./dist && tsc",
		"lint": "eslint . --ext .js, .ts --no-error-on-unmatched-pattern",
		"prettier": "prettier \"**/*.+(js|jsx|json|css|scss|ts|tsx|md|mdx|graphql)\"",
		"types": "tsc",
		"test": "jest",
		"format": "npm run prettier -- --write",
		"validate": "npm-run-all --parallel lint format types"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged && npm run validate"
		}
	},
	"lint-staged": {
		"*.+(js|jsx|ts|tsx)": [
			"eslint --fix"
		],
		"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
			"prettier --write",
			"jest --findRelatedTests"
		]
	},
	"keywords": [
		"javascript",
		"node"
	],
	"author": "George Bullock",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/georgebullock/react-interactive-dashboard/issues"
	},
	"homepage": "https://github.com/georgebullock/react-interactive-dashboard#readme",
	"dependencies": {
		"cookie-parser": "~1.4.4",
		"cross-env": "5.0.5",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"morgan": "~1.9.1"
	},
	"devDependencies": {
		"@types/express": "^4.17.6",
		"@types/express-serve-static-core": "^4.17.5",
		"@types/jest": "^25.2.1",
		"@types/node": "^13.11.1",
		"dotenv": "^8.2.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.1",
		"eslint-plugin-prettier": "^3.1.3",
		"helmet": "^3.22.0",
		"husky": "^4.2.5",
		"jest": "^25.3.0",
		"lint-staged": "^10.1.3",
		"nodemon": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.4",
		"rimraf": "^3.0.2",
		"ts-jest": "^25.4.0",
		"ts-node": "^8.8.2",
		"typescript": "^3.8.3"
	}
}
